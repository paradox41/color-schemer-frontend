{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "pretty": true,
    "removeComments": true,
    "target": "ES5"
  },
  "exclude": [
    "node_modules"
  ]
}
